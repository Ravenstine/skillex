#!/usr/bin/env node

'use strict';

const exec  = require('child_process').exec;

let skillName = process.argv[2];

console.log(`cp ${__dirname}/../blueprints ${process.cwd()}/${skillName}`)

exec(`cp ${__dirname}/../blueprints ${process.cwd()}/${skillName}`);

exec(`git init ${process.cwd()}/${skillName}`);

